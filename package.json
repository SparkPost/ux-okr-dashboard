{
  "name": "ux-okr-dashboard",
  "private": true,
  "description": "An OKR Dashboard for the SparkPost UX Team",
  "version": "0.1.0",
  "author": "SparkPost",
  "dependencies": {
    "@sparkpost/design-tokens": "^4.7.3",
    "@sparkpost/matchbox": "^4.7.3",
    "@sparkpost/matchbox-icons": "^4.7.3",
    "@vx/axis": "0.0.199",
    "@vx/group": "0.0.190",
    "@vx/scale": "0.0.190",
    "@vx/shape": "0.0.190",
    "@vx/tooltip": "0.0.199",
    "chalk": "^2.4.2",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-day-picker": "^7.4.8",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "glob": "^7.1.4",
    "next": "^10.0.5",
    "prettier": "^1.18.2",
    "typescript": "^3.9.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write \"src/**/*.{js,jsx,json,md}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "source:all": "npm run source:breakdown && npm run source:scss && npm run source:token && npm run source:size && npm run source:component",
    "source:scss": "sh ./scripts/scss.sh && node ./scripts/format-scss.js && prettier --write **/*.json",
    "source:token": "sh ./scripts/token.sh && prettier --write **/*.json",
    "source:size": "sh ./scripts/size.sh && prettier --write **/*.json",
    "source:component": "sh ./scripts/component.sh &&  prettier --write **/*.json",
    "source:breakdown": "node ./scripts/component-breakdown.js",
    "source:styled": "sh ./scripts/styled-component.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
